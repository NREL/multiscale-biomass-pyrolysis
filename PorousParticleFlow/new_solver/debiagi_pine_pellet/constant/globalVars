cylrad 0.003;
cylht 0.008;
cylmidr #calc "$cylrad*0.5";
cylmidz #calc "$cylht*0.5";
domx #calc "5.0*$cylrad";
domy #calc "5.0*$cylrad";
domz #calc "2.0*$cylht";
xmid #calc "$domx*0.5";
ymid #calc "$domy*0.5";
zmid #calc "$domz*0.5";
cyltopz #calc "$zmid+$cylmidz";
cylbotz #calc "$zmid-$cylmidz";
pore_size 1e-4;
cell_frac 0.398;
gmsw_frac 0.220;
ligh_frac 0.179;
ligo_frac 0.116;
tgl_frac 0.032;
moist_frac 0.055; 
